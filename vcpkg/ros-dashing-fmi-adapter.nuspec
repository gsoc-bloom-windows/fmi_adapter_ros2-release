<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-fmi-adapter</id>
    <version>0.1.5</version>
    <title>ros-dashing-fmi-adapter</title>
    <authors>Ralph Lange <ralph.lange@de.bosch.com></authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Wraps FMUs for co-simulation</description>
<dependencies>
      <dependency id="ros-dashing-launch"/>
      <dependency id="ros-dashing-launch-ros"/>
      <dependency id="ros-dashing-rcl-interfaces"/>
      <dependency id="ros-dashing-rclcpp"/>
      <dependency id="ros-dashing-rclcpp-components"/>
      <dependency id="ros-dashing-rclcpp-lifecycle"/>
      <dependency id="ros-dashing-std-msgs"/>
      <dependency id="ros-dashing-ros-workspace"/>
    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>